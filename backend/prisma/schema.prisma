datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Customer {
  id          Int      @id @default(autoincrement())
  name        String
  email       String   @unique
  hashed_pass String
  created_at  DateTime @default(now())
  updated_at  DateTime @updatedAt
}
